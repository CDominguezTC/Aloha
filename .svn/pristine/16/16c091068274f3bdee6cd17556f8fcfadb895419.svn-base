package Modelo;

/**
 *
 * @author Carlos A Dominguez D
 */
public class ModeloGrupoTurnos 
{
    int id;
    String codigo;
    String descripcion;

    public ModeloGrupoTurnos() {
    }

    public ModeloGrupoTurnos(int id, String codigo, String descripcion) {
        this.id = id;
        this.codigo = codigo;
        this.descripcion = descripcion;
    }

    public int getId() {
        return id;
    }

    public void setId(int id) {
        this.id = id;
    }

    public String getCodigo() {
        return codigo;
    }

    public void setCodigo(String codigo) {
        this.codigo = codigo;
    }

    public String getDescripcion() {
        return descripcion;
    }

    public void setDescripcion(String descripcion) {
        this.descripcion = descripcion;
    }    
}
