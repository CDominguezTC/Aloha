/*登录页面logo add by darcy 20140314*/
.loginLogo {
	background:url(/login/login_img/logo.gif) no-repeat scroll left top rgba(0, 0, 0, 0);
}
.loginLogoOem {
	background:url(/login/login_img/logo_oem.gif) no-repeat scroll left top rgba(0, 0, 0, 0);
}
/*内容页顶部左上角logo modify by darcy 20140314*/
.topLogo {
	height: 90px;
	padding-left:20px;
	background-color: #3C85C8;
	margin-left: 250px;
	/*background: url(../images/head/logo.png) no-repeat left top;*/
}
.topLogoOem {
	height: 81px;
	background: url(../images/head/top_logo_oem.jpg) no-repeat left top;
}

#logo .logo_biaoti {
	/*background: url(../images/head/top_biao_di.jpg) no-repeat left;*/
	height: 49px;
}
.l {
	float: left;
}
.r {
	float: right;
}

#logo .logo_biaoti_box {
	/*background: url(../images/head/top_biaobig_di.jpg) repeat-x;*/
	height: 49px;
	margin-left: 0px;
}
#logo .logo_an_box {
	/*background-image: url(../images/head/top_anbig_di.jpg);*/
	height: 40px;
	margin-left: 0px;
}
#logo .logo_biaoti span {
	font-size: 20px;
	line-height: 45px;
	color: #000;
	font-family: "Times New Roman", Times, serif;
	float: left;
}
#logo .logo_usfone {
	/*background: url(../images/head/top_us_di.jpg) repeat-x;*/
	height: 25px;
	padding-right: 20px;
	line-height: 20px;
	float: left;
}
#logo .logo_usfone strong{
	color: #FFFF00;
	vertical-align: middle;
	padding-right: 10px;
	padding-left: 10px;
	line-height: 24px;
	/*font-size: 12px;*/
	font-weight: normal;
}
.logo_usfone a:link,.logo_usfone a:visited,.logo_usfone a:active {
	color: #FFFFFF;
	text-decoration: none;
	padding-right: 10px;
	padding-left: 10px;
	line-height: 24px;
	/*background: url(../images/head/top_us_xian.jpg) no-repeat right center;*/
}

.logo_usfone a:hover {
	text-decoration: underline;
	color: #FFFFFF;
}
#logo .logo_biaoti_box .logo_ustu {
	/*background: url(../images/head/top_us_l.gif) no-repeat left top;*/
	float: left;
	height: 25px;
	width: 33px;
}
.logo_an_box ul {
	margin: 0px;
	padding: 0px;
}
.logo_an_boxli {
	/*background-image: url(../images/head/top_an_xianbian.jpg);*/
	background-color: #fff;
	background-repeat: repeat-x;
	background-position: top;
	border-right-width: 1px;
	border-left-width: 1px;
	border-right-style: solid;
	border-left-style: solid;
	border-right-color: #59abd6;
	border-left-color: #59abd6;
}

.logo_an_boxli a:link,.logo_an_boxli a:visited,.logo_an_boxli a:active {
	color: #000 !important;
	text-decoration: none;
}

.logo_an_boxli a:hover {
	text-decoration: underline;
	color: #FFFFFF;
}

.logo_an_box li {
	margin: 0px;
	float: left;
	list-style-type: none;
	line-height: 30px;
}

.logo_an_box a:link, .logo_an_box a:visited,.logo_an_box a:active{
	padding-top: 6px;
	padding-right: 10px;
	padding-bottom: 6px;
	padding-left: 10px;
	text-decoration: none;
	/* font-weight: bold;
	font-size: 14px; */

}

.logo_an_box li:hover {
	color: #FFFFFF;
	/*background-image: url(../images/head/top_an_xianbian1.jpg);*/
	background-repeat: repeat-x;
	background-position: top;
	
}
.logo_an_link,.logo_an_link a:link, .logo_an_link a:visited,.logo_an_link a:active .logo_an_link li:hover{
    color: #000 !important;
	text-decoration: none;

}
.logo_an_link li:hover{
    color: #fff !important;
	text-decoration: none;

}
.logo_an_box a:hover {
	color: #FFFFFF !important;
}

.logo_an_box .ico{
	background-repeat: no-repeat;
	height: 30px;
	width: 30px;
	float: left;
}
