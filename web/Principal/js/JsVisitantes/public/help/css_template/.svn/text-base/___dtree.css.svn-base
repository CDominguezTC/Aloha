@charset "utf-8";
html,body,input[type="text"],input[type="password"],input[type=null],select, td, th, button,form,span,p {
	font-family: Arial,微软雅黑 !important;/*tahoma, segoe ui light*/
}
.dtree {
	font-family: Arial,微软雅黑;
	font-size: 11px;
	color: #666;
	white-space: nowrap;
}
.dtree img {
	border: 0px;
	vertical-align: middle;
}
.dtree a {
	color: #333;
	text-decoration: none;
}
.dtree a.node, .dtree a.nodeSel {
	white-space: nowrap;
	padding: 1px 2px 1px 2px;
}
.dtree a.node:hover, .dtree a.nodeSel:hover {
	color: #333;
	text-decoration: underline;
}
.dtree a.nodeSel {
	background-color: #c0d2ec;
}
.dtree .clip {
	overflow: hidden;
}